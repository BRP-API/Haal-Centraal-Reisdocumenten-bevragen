  "components" : {
    "schemas" : {
      "Reisdocumentnummer" : {
        "pattern" : "^[a-zA-Z0-9]{9}$",
        "type" : "string"
      },
      "GbaReisdocument" : {
        "type" : "object",
        "properties" : {
          "reisdocumentnummer" : {
            "$ref" : "#/components/schemas/Reisdocumentnummer"
          },
          "soortReisdocument" : {
            "$ref" : "#/components/schemas/Waardetabel"
          },
          "datumEindeGeldigheid" : {
            "$ref" : "#/components/schemas/GbaDatum"
          },
          "datumInhoudingOfVermissing" : {
            "$ref" : "#/components/schemas/GbaDatum"
          },
          "aanduidingInhoudingOfVermissing" : {
            "$ref" : "#/components/schemas/AanduidingInhoudingOfVermissing"
          },
          "burgerservicenummer" : {
            "$ref" : "#/components/schemas/Burgerservicenummer"
          },
          "overlijden" : {
            "$ref" : "#/components/schemas/GbaOverlijden"
          }
        },
        "description" : "Gegevens van een reisdocument die nodig zijn om te bepalen of het reisdocument nog geldig en in het bezit van de houder is, plus de identificatie van de houder.\n"
      },
      "AanduidingInhoudingOfVermissing" : {
        "pattern" : "^(I|R|V|\\.)$",
        "type" : "string"
      },
      "Burgerservicenummer" : {
        "pattern" : "^[0-9]{9}$",
        "type" : "string",
        "example" : "555555021"
      },
      "GbaOverlijden" : {
        "type" : "object",
        "properties" : {
          "datum" : {
            "$ref" : "#/components/schemas/GbaDatum"
          }
        }
      },
      "BadRequestFoutbericht" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/Foutbericht"
        }, {
          "type" : "object",
          "properties" : {
            "invalidParams" : {
              "type" : "array",
              "description" : "Foutmelding per fout in een parameter. Alle gevonden fouten worden één keer teruggemeld.",
              "items" : {
                "$ref" : "#/components/schemas/InvalidParams"
              }
            }
          }
        } ]
      },
      "Foutbericht" : {
        "type" : "object",
        "properties" : {
          "type" : {
            "type" : "string",
            "description" : "Link naar meer informatie over deze fout",
            "format" : "uri"
          },
          "title" : {
            "pattern" : "^[a-zA-Z0-9À-ž \\.\\-]{1,80}$",
            "type" : "string",
            "description" : "Beschrijving van de fout"
          },
          "status" : {
            "maximum" : 600,
            "minimum" : 100,
            "type" : "integer",
            "description" : "Http status code"
          },
          "detail" : {
            "pattern" : "^[a-zA-Z0-9À-ž \\.\\-\\(\\)\\,]{1,200}$",
            "type" : "string",
            "description" : "Details over de fout"
          },
          "instance" : {
            "type" : "string",
            "description" : "Uri van de aanroep die de fout heeft veroorzaakt",
            "format" : "uri"
          },
          "code" : {
            "minLength" : 1,
            "pattern" : "^[a-zA-Z0-9]{1,25}$",
            "type" : "string",
            "description" : "Systeemcode die het type fout aangeeft"
          }
        },
        "description" : "Terugmelding bij een fout. JSON representatie in lijn met [RFC7807](https://tools.ietf.org/html/rfc7807)."
      },
      "InvalidParams" : {
        "type" : "object",
        "properties" : {
          "type" : {
