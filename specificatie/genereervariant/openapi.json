  "components" : {
    "schemas" : {
      "Reisdocumentnummer" : {
        "pattern" : "^[a-zA-Z0-9]{9}$",
        "type" : "string"
      },
      "Reisdocument" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/ReisdocumentBasis"
        }, {
          "type" : "object",
          "properties" : {
            "inOnderzoek" : {
              "$ref" : "#/components/schemas/ReisdocumentInOnderzoek"
            }
          },
          "description" : "Een document dat nodig is voor reizen naar het buitenland.\n\nindicatieGeldig geeft aan of het reisdocument op dit moment nog geldig en in het bezit van de persoon is. Wordt gevuld met waarden voor 'Geldig_Reisdocument' in 'tabelwaarden.csv'.\n"
        } ]
      },
      "ReisdocumentBasis" : {
        "type" : "object",
        "properties" : {
          "reisdocumentnummer" : {
            "$ref" : "#/components/schemas/Reisdocumentnummer"
          },
          "indicatieGeldig" : {
            "$ref" : "#/components/schemas/Waardetabel"
          },
          "burgerservicenummer" : {
            "$ref" : "#/components/schemas/Burgerservicenummer"
          },
          "geheimhoudingPersoongegevens" : {
            "$ref" : "#/components/schemas/GeheimhoudingPersoonsgegevens"
          },
          "opschortingBijhouding" : {
            "$ref" : "#/components/schemas/OpschortingBijhouding"
          }
        },
        "description" : "Een document dat nodig is voor reizen naar het buitenland.\n\nindicatieGeldig geeft aan of het reisdocument op dit moment nog geldig en in het bezit van de persoon is. Wordt gevuld met waarden voor 'Geldig_Reisdocument' in 'tabelwaarden.csv'.\n"
      },
      "Burgerservicenummer" : {
        "pattern" : "^[0-9]{9}$",
        "type" : "string",
        "example" : "555555021"
      },
      "GeheimhoudingPersoonsgegevens" : {
        "type" : "boolean",
        "description" : "Gegevens mogen niet worden verstrekt aan derden / maatschappelijke instellingen.\n"
      },
      "ReisdocumentInOnderzoek" : {
        "description" : "Geeft aan welke gegevens over het reisdocument in onderzoek zijn.\n",
        "allOf" : [ {
          "$ref" : "#/components/schemas/InOnderzoek"
        }, {
          "type" : "object",
          "properties" : {
            "burgerservicenummer" : {
              "type" : "boolean"
            },
            "reisdocumentnummer" : {
              "type" : "boolean"
            },
            "indicatieGeldig" : {
              "type" : "boolean"
            }
          }
        } ]
      },
      "InOnderzoek" : {
        "type" : "object",
        "properties" : {
          "datumIngangOnderzoek" : {
            "$ref" : "#/components/schemas/AbstractDatum"
          }
        }
      },
      "BadRequestFoutbericht" : {
        "allOf" : [ {
          "$ref" : "#/components/schemas/Foutbericht"
        }, {
          "type" : "object",
          "properties" : {
            "invalidParams" : {
              "type" : "array",
              "description" : "Foutmelding per fout in een parameter. Alle gevonden fouten worden één keer teruggemeld.",
              "items" : {
                "$ref" : "#/components/schemas/InvalidParams"
              }
            }
          }
        } ]
      },
      "Foutbericht" : {
        "type" : "object",
        "properties" : {
          "type" : {
            "type" : "string",
            "description" : "Link naar meer informatie over deze fout",
            "format" : "uri"
          },
          "title" : {
            "pattern" : "^[a-zA-Z0-9À-ž \\.\\-]{1,80}$",
            "type" : "string",
            "description" : "Beschrijving van de fout"
          },
          "status" : {
